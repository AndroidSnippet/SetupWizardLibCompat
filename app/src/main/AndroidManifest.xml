<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="top.trumeet.snippet.setupwizardlibcompat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".SetupWizardLayoutActivity"
            android:theme="@style/SuwThemeMaterial.Light"
            android:label="SetupWizardLayoutDemo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GlifLayoutActivity"
            android:theme="@style/SuwThemeGlif.Light"
            android:label="GlifLayoutDemo"/>
    </application>
</manifest>
